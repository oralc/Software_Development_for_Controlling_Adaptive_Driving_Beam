%%            Eidesstattliche Erklärung
%%----------------------------------------------------------------------
\newcommand{\@erklaerung}
{
	\renewcommand*\chapterheadstartvskip{\vspace*{4\topskip}}
	\chapter{Sworn declaration}	
	\begin{textblock*}{15cm}(3.5cm,1.25cm)
		\begin{flushright}
			\includegraphics[width=6.25cm]{Grafik/FH.png}
		\end{flushright}
		\rule{15cm}{0.1mm}
		\vspace{-2.2cm}
		\begin{flushright}
			Wels Campus
		\end{flushright}
	\end{textblock*}
	
	\vspace{5cm}
	
	\begin{minipage}[t]{0.33\textwidth}
	\end{minipage}
	\hfill
	\begin{minipage}[t]{0.66\textwidth}
		\noindent
		I hereby declare that I prepared this work independently and without help from third parties, that I did not use sources other than the ones referenced and that I have indicated passages taken from those sources.
		\vskip 2mm
		This thesis was not previously submitted in identical or similar form to any other examination board, nor was it published.
		\vskip 2mm
		This printed thesis is identical with the electronic version submitted.

		\vspace{25mm}
		\begin{minipage}[t]{7.5cm}
			\centering \dotfill \\
		\end{minipage}
		\vskip 0mm
		\@author
		\vskip 0mm
		\@stdort, {\@damnt\ \@dajhr}
		% Unterschrift anzeigen:
		\ifthenelse{\equal{\@showSig}{Yes}}
			{%
				\vskip \@posSigY
				\hskip \@posSigX
				\includegraphics[scale=\@scaleSig]{Grafik/Signature.png}%
			}%
			{}
	\end{minipage}
}
